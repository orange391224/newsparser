<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="ttk.Labelframe" id="mainwindow">
    <property name="height">200</property>
    <property name="width">220</property>
    <layout>
      <property name="column">0</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
    </layout>
    <child>
      <object class="ttk.Frame" id="TreeFrame">
        <property name="height">200</property>
        <property name="padding">0</property>
        <property name="width">400</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">1</property>
          <property name="ipadx">20</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="rowspan">6</property>
          <property name="sticky">nsw</property>
        </layout>
        <child>
          <object class="ttk.Button" id="id_down_btn">
            <property name="command">id_down_trigger</property>
            <property name="image">down-arrow.png</property>
            <property name="text" translatable="yes">Вниз</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="sticky">se</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="id_up_btn">
            <property name="command">id_up_trigger</property>
            <property name="image">up-arrow.png</property>
            <property name="text" translatable="yes">Вверх</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="sticky">sw</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="delete_tree_btn">
            <property name="command">delete_tree</property>
            <property name="image">delete.png</property>
            <property name="text" translatable="yes">Удалить</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="sticky">s</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Treeview" id="Treeview_1">
            <property name="height">30</property>
            <property name="padding">0</property>
            <property name="selectmode">extended</property>
            <property name="show">tree</property>
            <layout>
              <property name="column">0</property>
              <property name="ipadx">90</property>
              <property name="propagate">True</property>
              <property name="row">2</property>
              <property name="sticky">nsw</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Scrollbar" id="Scrollbar_3">
            <property name="orient">vertical</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">2</property>
              <property name="sticky">nse</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="url_btns_frame">
        <property name="height">200</property>
        <property name="padding">1</property>
        <property name="takefocus">false</property>
        <property name="width">200</property>
        <layout>
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">n</property>
        </layout>
        <child>
          <object class="ttk.Separator" id="Separator_1">
            <property name="orient">horizontal</property>
            <layout>
              <property name="column">0</property>
              <property name="ipady">5</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="url_entry">
            <property name="width">68</property>
            <layout>
              <property name="column">1</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
