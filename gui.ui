              <property name="column">3</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="url_label">
            <property name="text" translatable="yes">URL:</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="alt_load_btn">
            <property name="command">alt_loading</property>
            <property name="text" translatable="yes">Альт.загр</property>
            <layout>
              <property name="column">5</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Separator" id="Separator_4">
            <property name="orient">horizontal</property>
            <layout>
              <property name="column">6</property>
              <property name="ipadx">15</property>
              <property name="padx">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="sticky">ew</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="append_btn">
            <property name="command">attach</property>
            <property name="text" translatable="yes">Вставь!</property>
            <layout>
              <property name="column">7</property>
              <property name="pady">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="warning_label_frame">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout>
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
        </layout>
        <child>
          <object class="ttk.Label" id="warning_label">
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="title_frame">
        <property name="height">200</property>
        <property name="padding">1</property>
        <property name="width">200</property>
        <layout>
          <property name="column">1</property>
          <property name="padx">20</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
          <property name="sticky">ew</property>
        </layout>
        <child>
          <object class="ttk.Label" id="title_label">
            <property name="compound">top</property>
            <property name="cursor">arrow</property>
            <property name="text" translatable="yes">Заголовок</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="sticky">w</property>
            </layout>
          </object>
